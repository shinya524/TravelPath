<div class ="container">
  <div class ="row">
    <h4>いいね一覧</h4>
  </div>

  <div class ="row">
    <table class ="table">
      <tbody>
        <% @favorite_post_spots.each do |post_spot|%>
        <tr>
          <td>
            <%= link_to post_spot_path(post_spot.id) do %>
              <%= post_spot.name %>
            <% end %>
          </td>
          <td><%= post_spot.user.name %></td>
        </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>